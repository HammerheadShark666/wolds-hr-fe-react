{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/assets/*"]
  },
  "routes": [
    {
      "route": "/",
      "redirect": "https://www.wolds-hr.co.uk",
      "statusCode": 301
    },
    {
      "route": "/login",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ]
}